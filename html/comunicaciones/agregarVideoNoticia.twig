<div class="modal-content">
	<div class="modal-header">
	  <button type="button" class="close" ng-click="cancel()"></button>
	  <h4 class="modal-title">Agregar Video</h4>
	</div>


		<div class="modal-body"  >
			<table>
			<div class="subirFoto">
				    		[% import 'subirVideo.twig' as subirVideo%]
							[[ subirVideo.input() ]]
							<div class="clear"></div>
			</div>
			<input type="hidden" ng-model="form.id" ng-init="form.id = '[[id]]' "/>
			<input type="hidden" ng-model="form.carpeta" ng-init="form.carpeta = 'noticia' "/>
			<input type="hidden" ng-model="form.section" ng-init="form.section = 'agregarVideo' "  name="section" >
			</table>	
		</div>
		<div class="modal-footer">
				<div class="">
		      		<button type="button" class="btnOutline cancelar marginR btn-default" ng-click="cancel()">Cancelar</button>
		      		<button type="button" ng-click="submitForm('[[ paths.PATH_CONTROLLER_COMUNICACIONES ]]controladorAdminModel.php?language=[[ lang ]]', form)" class="btn publicar btn-primary" >Publicar</button>
		      	</div>
		 </div>
</div>